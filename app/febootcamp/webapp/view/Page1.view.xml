<mvc:View
    controllerName="febootcamp.controller.Page1"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:viz="sap.viz.ui5.controls" 
    xmlns:viz.data="sap.viz.ui5.data"
    xmlns:layout="sap.ui.layout"
    xmlns:viz.feeds="sap.viz.ui5.controls.common.feeds">

    <Page id="page1" title="Page 1">
        <content>
          <Title id="page1title" text="Hello, Roro!" class="page1title" />
          <Button
					id="BadgedButton"
					class="sapUiTinyMarginBeginEnd"
					icon="sap-icon://paper-plane"
					text="Click to say Hello!"
          press="onButtonPress"/>
          
          <Panel id="Books" class="panelStyle" expandable="true" expanded="true" headerText="Book Chart" width="auto">
          <viz:VizFrame id="idVizFrame"
                  uiConfig="{applicationSet:'page1'}"
                  height="300px"
                  width="100%"
                  vizType="column"
                  vizProperties="{ title: { text: 'Books Data', visible: true } }">
        <viz:dataset>
            <viz.data:FlattenedDataset id="idVizDataset" data="{viz>/SalesData}">
                <viz.data:dimensions>
                    <viz.data:DimensionDefinition id="idDimMonth" name="Month" value="{viz>Month}" />
                </viz.data:dimensions>
                <viz.data:measures>
                    <viz.data:MeasureDefinition id="idMeasCreatedClubs" name="Created Clubs" value="{viz>Value}" />
                </viz.data:measures>
            </viz.data:FlattenedDataset>
        </viz:dataset>
        <viz:feeds>
            <viz.feeds:FeedItem id="idFeedValueAxis" uid="valueAxis" type="Measure" values="Created Clubs" />
            <viz.feeds:FeedItem id="idFeedCategoryAxis" uid="categoryAxis" type="Dimension" values="Month" />
        </viz:feeds>
    </viz:VizFrame>
          
          </Panel>  
        <Panel id="Books1" class="panelStyle" expandable="true" expanded="true" headerText="List" width="auto">
                </Panel> 
           
        </content>
         <footer>
            <Toolbar id="footerToolbar">
                <Button
                    id="nextButton"
                    text="Click Me to go to Page 2!"
                    press="onGoToPage2"
                />
            </Toolbar>
        </footer>
    </Page>
</mvc:View>
